{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "color-tools",
  "title": "Color Tools",
  "description": "Color tools for Raycast. Color picker, color from hex code, convert between color formats, etc.",
  "icon": "command-icon.png",
  "author": "MatthewStanciu",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Hello World",
      "description": "Template with a plain detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}